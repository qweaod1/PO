lista = []

for x in range(1,11):
    lista.append(x)

print(lista)

lista1 =[]

for x in range(0,21):
    if x%2==0:
        lista1.append(x)

print(lista1)

lista3 = []

for x in range(1,11):
    lista3.append(x*x)

print(lista3)

lista4 = []

for x in range(1,11):
    lista4.append(0)

print(lista4)

lista5 = []

for x in range(2,12):
    lista5.append(x%2)

print(lista5)

lista6 = []

for x in range(0,10):
    lista6.append(x%5)

print(lista6)



